//Lantai 4
void lantai4() {
    glPushMatrix();
    glTranslatef(0, 170, 0);
    glColor3f(1, 0.31, 0);
    glutSolidCube(31.0);
    glPopMatrix();

    glPushMatrix();
    glTranslatef(0, 117, 0);
    glScalef(0.6, 0.6, 0.6);


    //Balkon
    glBegin(GL_POLYGON);
    glColor3f(0.77, 0.24, 0);
    glVertex3f(-50, 70, -50);
    glVertex3f(50, 70, -50);
    glColor3f(1, 0.31, 0);
    glVertex3f(50, 70, 50);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(-50, 70, -50);
    glVertex3f(50, 70, -50);
    glVertex3f(50, 66, -50);
    glVertex3f(-50, 66, -50);
    glVertex3f(-50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(50, 70, -50);
    glVertex3f(50, 70, 50);
    glVertex3f(50, 66, 50);
    glVertex3f(50, 66, -50);
    glVertex3f(50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(50, 70, 50);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 66, 50);
    glVertex3f(50, 66, 50);
    glVertex3f(50, 70, 50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 70, -50);
    glVertex3f(-50, 66, -50);
    glVertex3f(-50, 66, 50);
    glVertex3f(-50, 70, 50);
    glEnd();

    glPopMatrix();
}


void atap4_kiri() {
    glPushMatrix();
    glTranslatef(0, 105, 0);
    glScalef(0.7, 0.7, 0.7);

    glBegin(GL_POLYGON);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-5, 140, 5);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-5, 140, -5);
    glVertex3f(-25, 121, -25);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-25, 121, 25);
    glEnd();

    //1
    glBegin(GL_POLYGON);
    glColor3f(0, 0.79, 0.79); 
    glVertex3f(-25, 121, 25);
