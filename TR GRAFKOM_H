void atap2_kiri() {
    glPushMatrix();
    glTranslatef(0, -3, 0);
    //1
    glBegin(GL_POLYGON);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-25, 121, 25);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-25, 121, -25);
    glVertex3f(-35, 117.5, -35.21);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-35, 117.5, 35.21);
    glVertex3f(-25, 121, 25);
    glEnd();

    //2
    glBegin(GL_POLYGON);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-35, 117.5, -35.21);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-35, 117.5, 35.21);
    glVertex3f(-40, 116, 40.21);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-40, 116, -40.21);
    glEnd();

    //3
    glBegin(GL_POLYGON);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-40, 116, 40.21);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-40, 116, -40.21);
    glVertex3f(-45, 115, -45.21);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-45, 115, 45.21);
    glEnd();

    //4
    glBegin(GL_POLYGON);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-45, 115, -45.21);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-45, 115, 45.21);
    glVertex3f(-50, 115.5, 50);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-50, 115.5, -50);
    glEnd();

    glPopMatrix();
}

void atap2_belakang() {
    glPushMatrix();
    glRotated(90, 0, -200, 0);
    glTranslatef(0, -3, 0);

    //1
    glBegin(GL_POLYGON);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-25, 121, 25);
    glVertex3f(-25, 121, -25);
    glVertex3f(-35, 117.5, -35.21);
    glVertex3f(-35, 117.5, 35.21);
    glVertex3f(-25, 121, 25);
    glEnd();

    //2
    glBegin(GL_POLYGON);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-35, 117.5, -35.21);
    glVertex3f(-35, 117.5, 35.21);
    glVertex3f(-40, 116, 40.21);
    glVertex3f(-40, 116, -40.21);
    glEnd();

    //3
    glBegin(GL_POLYGON);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-40, 116, 40.21);
    glVertex3f(-40, 116, -40.21);
    glVertex3f(-45, 115, -45.21);
    glVertex3f(-45, 115, 45.21);
    glEnd();

    //4
    glBegin(GL_POLYGON);
    glColor3f(0, 0.54, 0.54);
    glVertex3f(-45, 115, -45.21);
    glVertex3f(-45, 115, 45.21);
    glVertex3f(-50, 115.5, 50);
    glVertex3f(-50, 115.5, -50);
    glEnd();

    glPopMatrix();
}

void atap2_kanan() {
