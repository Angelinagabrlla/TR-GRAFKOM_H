void pagar_gabung1() {

    pager();
    pagar_cilik();
    //kanan
    glPushMatrix();
    glTranslatef(94.5, 0, 0);
    pager();
    pagar_cilik();
    glPopMatrix();
    //depan
    glPushMatrix();
    glRotated(90, 0, 10, 0);
    pager();
    pagar_cilik();
    glPopMatrix();
    //belakang
    glPushMatrix();
    glRotated(-90, 0, 10, 0);
    pager();
    pagar_cilik();
    glPopMatrix();
}

void pagar_gabung2() {
    glPushMatrix();
    glTranslatef(0, 83, 0);
    glScalef(0.55, 0.55, 0.55);
    pagar_gabung1();
    glPopMatrix();
}

void pagar_gabung3() {
    glPushMatrix();
    glTranslatef(0, 116.5, 0);
    glScalef(0.6, 0.6, 0.6);
    pagar_gabung1();
    glPopMatrix();
}

void lantai1() {
    glPushMatrix();
    glTranslatef(0, 30, 0);
    glColor3f(1, 0.31, 0);
    glutSolidCube(80.0);
    glPopMatrix();

    glPushMatrix();
    glTranslatef(0, 1, 0);

    //Balkon
    glBegin(GL_POLYGON);
    glColor3f(0.77, 0.24, 0);
    glVertex3f(-50, 70, -50);
    glVertex3f(50, 70, -50); 
    glColor3f(1, 0.31, 0);
    glVertex3f(50, 70, 50);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(-50, 70, -50);
    glVertex3f(50, 70, -50);
    glVertex3f(50, 66, -50);
    glVertex3f(-50, 66, -50);
    glVertex3f(-50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(50, 70, -50);
    glVertex3f(50, 70, 50);
    glVertex3f(50, 66, 50);
    glVertex3f(50, 66, -50);
    glVertex3f(50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(50, 70, 50);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 66, 50);
    glVertex3f(50, 66, 50);
    glVertex3f(50, 70, 50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 70, -50);
    glVertex3f(-50, 66, -50);
    glVertex3f(-50, 66, 50);
    glVertex3f(-50, 70, 50);
    glEnd();

    glPopMatrix();


}

