    glVertex3f(-20.0, 40.0, 50);
    glVertex3f(-17.0, 40.0, 50);
    glVertex3f(-17.0, -20.0, 130);
    glVertex3f(-20.0, -20.0, 130);
    glVertex3f(-20.0, 40.0, 50);
    glEnd();
}

//Lantai 1
void pager() {

    
    //tiangkemu
    glPushMatrix();
    glColor3f(0.77, 0.24, 0);
    glTranslatef(-47.5, 79, -48);
    gluCylinder(gluNewQuadric(), 0.5f, 0.5f, 96.0f, 32, 32);
    glPopMatrix();
    glPushMatrix();
    glColor3f(0.77, 0.24, 0);
    glTranslatef(-47.5, 76, -48);
    gluCylinder(gluNewQuadric(), 0.5f, 0.5f, 96.0f, 32, 32);
    glPopMatrix();

    //tiangsae
    glPushMatrix();
    glColor3f(0.77, 0.24, 0);
    glRotated(90, -5.5, 1, 1);
    glTranslatef(-60.5, -25.5, 72);
    gluCylinder(gluNewQuadric(), 1.0f, 1.0f, 8.0f, 32, 32);
    glPopMatrix();

    //tiangsae1
    glPushMatrix();
    glColor3f(0.77, 0.24, 0);
    glRotated(90, -5.5, 1, 1);
    glTranslatef(-27.5, 63.5, 69);
    gluCylinder(gluNewQuadric(), 1.0f, 1.0f, 8.0f, 32, 32);
    glPopMatrix();

}



