    glTranslatef(-4.5, 0, 0);
    //jendela
    glPushMatrix();
    glTranslatef(13, 129, -19);
    glColor3f(1.0, 1.0, 1.0);
    glBegin(GL_POLYGON);
    glVertex3f(-3.0, 0.0, 0.0);
    glVertex3f(3.0, 0.0, 0.0);
    glVertex3f(3.0, 8.0, 0.0);
    glVertex3f(-3.0, 8.0, 0.0);
