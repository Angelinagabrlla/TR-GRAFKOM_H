    //4
    glBegin(GL_POLYGON);
    glColor3f(0, 0.79, 0.79);
    glVertex3f(-45, 115, -45.21);
    glVertex3f(-45, 115, 45.21);
    glVertex3f(-50, 115.5, 50);
    glVertex3f(-50, 115.5, -50);
    glEnd();

    glPopMatrix();
}

void pucuk() {
    //tabungkeatas
    glPushMatrix();
    glColor3f(0.99, 0.86, 0);
    glRotated(90, -5.5, 1, 1);
    glTranslatef(1, 6.5, 206.5);
    gluCylinder(gluNewQuadric(), 0.5f, 0.5f, 15.0f, 32, 32);
    glPopMatrix();
    //pucukan
    glPushMatrix();
    glColor3f(0.99, 0.86, 0);
    glRotated(90, -5.5, 1, 1);
    glTranslatef(1, 6.5, 221.5);
    glutSolidCone(0.8, 5.0, 20, 20);
    glPopMatrix();
    //bulatan1
