    glVertex3f(17.0, 40.0, 50);
    glVertex3f(17.0, -20.0, 130);
    glVertex3f(20.0, -20.0, 130);
    glVertex3f(20.0, 40.0, 50);
    glEnd();

    //Kiri
    glBegin(GL_POLYGON);
    glColor3f(0.27, 0.27, 0.27);
    glVertex3f(-20.0, -20.0, 130);
    glVertex3f(-20.0, 40.0, 50);
    glVertex3f(-20.0, -20.0, 50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.27, 0.27, 0.27);
    glVertex3f(-17.0, -20.0, 130);
    glVertex3f(-17.0, 40.0, 50);
    glVertex3f(-17.0, -20.0, 50);
    glEnd();

    glBegin(GL_POLYGON);
