//Lantai 3
void lantai3() {
    glPushMatrix();
    glTranslatef(0, 140, 0);
    glColor3f(1, 0.31, 0);
    glutSolidCube(37.0);
    glPopMatrix();

    glPushMatrix();
    glTranslatef(0, 82, 0);
    glScalef(0.57, 0.57, 0.57);

    //Balkon
    glBegin(GL_POLYGON);
    glColor3f(0.77, 0.24, 0);
    glVertex3f(-50, 70, -50);
    glVertex3f(50, 70, -50);
    glColor3f(1, 0.31, 0);
    glVertex3f(50, 70, 50);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(-50, 70, -50);
    glVertex3f(50, 70, -50);
    glVertex3f(50, 66, -50);
    glVertex3f(-50, 66, -50);
    glVertex3f(-50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(50, 70, -50);
    glVertex3f(50, 70, 50);
    glVertex3f(50, 66, 50);
    glVertex3f(50, 66, -50);
    glVertex3f(50, 70, -50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(50, 70, 50);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 66, 50);
    glVertex3f(50, 66, 50);
    glVertex3f(50, 70, 50);
    glEnd();

    glBegin(GL_POLYGON);
    glColor3f(0.99, 0.86, 0);
    glVertex3f(-50, 70, 50);
    glVertex3f(-50, 70, -50);
    glVertex3f(-50, 66, -50);
    glVertex3f(-50, 66, 50);
    glVertex3f(-50, 70, 50);
    glEnd();

    glPopMatrix();
}

void atap3_kiri() {
    glPushMatrix();
